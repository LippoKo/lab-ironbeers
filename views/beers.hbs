<section class='container'>
    {{#each data}}
  <div class='beers-div'>
      <img src='{{this.image_url}}' alt='beers' />
      <p>{{this.name}}</p>
      <p>{{this.description}}</p>
      <p>{{this.tagLine}}</p>
      {{#each this.food_pairing}}
        <p>{{this}}</p>
      {{/each}}
      <p>{{this.brewers_tips}}</p>
    {{/each}}
  </div>
</section>